    \u0967\u096C\u096B\u096E \u0924\u0915 \u0936\u093E\u0938\u0928 \u0915\u093F\u092F\u093E
    \u0917\u092F\u093E) \u0928\u0947 \u0905\u092A\u0928\u0940 \u092A\u0938\u0902\u0926\u0940\u0926\u093E
    \u092A\u0924\u094D\u0928\u0940 \u092E\u0941\u092E\u0924\u093E\u091C\u093C \u092E\u0939\u0932
    \u0915\u0940 \u0915\u092C\u094D\u0930 \u0915\u0947 \u0932\u093F\u090F \u0915\u092E\u0940\u0936\u0928
    \u0915\u093F\u092F\u093E \u0925\u093E; \u0907\u0938\u092E\u0947\u0902 \u0938\u094D\u0935\u092F\u0902
    \u0936\u093E\u0939\u091C\u0939\u093E\u0901 \u0915\u093E \u092E\u0915\u092C\u0930\u093E
    \u092D\u0940 \u0939\u0948\u0964 \u092E\u0915\u092C\u0930\u093E 17-\u0939\u0947\u0915\u094D\u091F\u0947\u092F\u0930
