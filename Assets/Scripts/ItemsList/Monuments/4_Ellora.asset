    \u0905\u0916\u0902\u0921 \u0930\u0949\u0915 \u0909\u0924\u094D\u0916\u0928\u0928,
    \u0915\u0948\u0932\u093E\u0936 \u092E\u0902\u0926\u093F\u0930, \u092D\u0917\u0935\u093E\u0928
