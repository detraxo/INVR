    \u092A\u094D\u0932\u093E\u0938\u094D\u091F\u0930 \u0915\u0940 \u092A\u0930\u0924
    \u0938\u0942\u0916\u0940 \u0925\u0940\u0964 \u0938\u092C\u0938\u0947 \u092A\u0939\u0932\u0947,
    \u092E\u093F\u091F\u094D\u091F\u0940, \u0917\u093E\u092F \u0915\u0947 \u0917\u094B\u092C\u0930
    \u0914\u0930 \u091A\u093E\u0935\u0932 \u0915\u0940 \u092D\u0942\u0938\u0940 \u0915\u093E
    \u090F\u0915 \u092E\u094B\u091F\u093E \u092A\u094D\u0932\u093E\u0938\u094D\u091F\u0930
    \u0917\u0941\u092B\u093E \u0915\u0940 \u0926\u0940\u0935\u093E\u0930\u094B\u0902
    \u092A\u0930 \u0926\u092C\u093E\u092F\u093E \u0917\u092F\u093E \u0925\u093E\u0964
