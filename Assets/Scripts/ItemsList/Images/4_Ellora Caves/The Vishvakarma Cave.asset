    \u0939\u0949\u0932 \u091C\u093F\u0938\u0947 '\u0935\u093F\u0936\u094D\u0935\u0915\u0930\u094D\u092E\u093E
    \u0917\u0941\u092B\u093E' \u0915\u0939\u093E \u091C\u093E\u0924\u093E \u0939\u0948,
    \u091C\u093F\u0938\u0947 650 \u0938\u0940\u0908 \u0915\u0947 \u0906\u0938\u092A\u093E\u0938
    \u092C\u0928\u093E\u092F\u093E \u0917\u092F\u093E \u0925\u093E\u0964 \u092C\u0940\u092E
