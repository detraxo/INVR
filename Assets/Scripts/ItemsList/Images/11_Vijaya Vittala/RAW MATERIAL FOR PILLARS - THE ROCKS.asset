    \u0938\u094D\u0925\u093F\u0930 \u092D\u094C\u0917\u094B\u0932\u093F\u0915 \u0938\u0902\u0930\u091A\u0928\u093E\u0913\u0902
    \u092E\u0947\u0902 \u0938\u0947 \u090F\u0915 \u0939\u0948\u0964 \u092E\u0948\u0917\u094D\u092E\u0948\u091F\u093F\u0915
    \u0915\u094D\u0930\u093F\u092F\u093E \u0915\u0947 \u0915\u093E\u0930\u0923 \u092A\u0941\u0930\u093E\u0928\u0947
